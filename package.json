	"devDependencies": {
		"@biomejs/biome": "^2.2.5",
		"@types/node": "^20.12.11",
		"semantic-release": "^24.2.9",
		"serve": "^14.2.3",
		"tsx": "^4.20.6",
		"typescript": "^5.4.5"
